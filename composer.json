{
    "name": "ferreiramg/clicksign",
    "description": "SDK para integrar Clicksign em Laravel",
    "type": "library",
    "keywords": [
        "laravel",
        "clicksign",
        "signature",
        "digital-signature",
        "sdk"
    ],
    "homepage": "https://github.com/Ferreiramg/clicksign",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.8",
        "laravel/pint": "^1.23",
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Clicksign\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ferreiramg",
            "email": "your-email@example.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Clicksign\\ClicksignServiceProvider"
            ],
            "aliases": {
                "Clicksign": "Clicksign\\Facades\\Clicksign"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "pest",
        "test-coverage": "pest --coverage",
        "format": "pint",
        "format-check": "pint --test"
    }
}