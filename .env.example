# Exemplo de configuração .env para Clicksign API v3

# Token de acesso da API Clicksign
CLICKSIGN_ACCESS_TOKEN=your_access_token_here

# URL base da API (produção)
CLICKSIGN_BASE_URL=https://app.clicksign.com/api/v3

# Modo sandbox (true para testes, false para produção)
CLICKSIGN_SANDBOX=true

# URL do sandbox
CLICKSIGN_SANDBOX_URL=https://sandbox.clicksign.com/api/v3

# Secret para verificação de webhooks
CLICKSIGN_WEBHOOK_SECRET=your_webhook_secret_here

# Timeout para requisições HTTP (em segundos)
CLICKSIGN_TIMEOUT=30
